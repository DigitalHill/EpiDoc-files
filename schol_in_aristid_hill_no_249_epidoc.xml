				<s n="3"><w n="1">Σωκράτης</w> <w n="2">Ἀναγυράσιος</w><w n="3">,</w> <w n="4">Σοφοκλῆς</w> <w n="5">ἐκ</w> <w n="6">Κολωνοῦ</w> <w n="7">ὁ</w> <w n="8">ποιητής</w><w n="9">,</w> <w n="10">Ἀνδοκίδης</w> <w n="11">Κυδαθηναιεὺς</w><w n="12">,</w> <w n="13">Κρέων</w> <w n="14">Σκαμβωνίδης</w><w n="15">,</w> <w n="16">Περικλῆς</w> <w n="17">Χολαργεύς</w><w n="18">,</w> <w n="19">Γλαύκων</w> <w n="20">ἐκ</w> <w n="21">Κεραμέων</w><w n="22">,</w> <w n="23">Καλλίστρατος</w> <w n="24">Ἀχαρνεύς</w><w n="25">,</w> <w n="26">Ξενοφῶν</w> <w n="27">Μελιτεύς</w><w n="28">.</w></s>
