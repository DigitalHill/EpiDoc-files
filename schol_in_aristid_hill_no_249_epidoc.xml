				<s n="1"><w n="1">ἐπι</w> <w n="2">μὲν</w> <w n="3">Σάμῳ</w> <w n="4">δέκατος</w> <w n="5">αὐτὸς</w> <w n="6">στρατηγῶν</w><w n="7">·</w></s> 
